{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "es2015", "es5", "es6"],
    "module": "commonjs",
    "declaration": true,
    "noImplicitAny": false,
    "outDir": "./lib",
    "strict": true,
    "jsx": "react-jsx",
    "skipLibCheck": true,
    "typeRoots": [
      "node_modules/@types",
      "../../node_modules/@types"
    ]
  },
  "include": ["src"],
  "exclude": ["node_modules", "lib"],
  "types": [
    "@s-ui/react-router/src/types.d.ts"
  ]
}